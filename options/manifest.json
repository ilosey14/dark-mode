{
    "manifest_version": 2,
    "name": "Options",
    "version": "0.2.1",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "options.js"
            ]
        }
    ],
    "options_ui": {
        "page": "options.html"
    },
    "permissions": [
        "storage"
    ]
}
